{
  "name": "emooneeds",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build": "npx tsc",
    "start:dev": "env-cmd -f ./.env.dev ts-node index.ts",
    "start:qa": "env-cmd -f ./.env.qa ts-node index.ts",
    "start:uat": "env-cmd -f ./.env.uat ts-node index.ts",
    "start:prod": "env-cmd -f ./.env.prod ts-node index.ts",
    "serve": "env-cmd nodemon --watch 'src/**/*.ts' --watch 'src/**/*.json' --watch index.ts --exec ts-node index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check **/*.ts",
    "format:fix": "prettier --write **/*.ts",
    "test": "env-cmd jest -i --verbose --detectOpenHandles",
    "test:debug": "env-cmd node --expose-gc --max-old-space-size=6144 ./node_modules/.bin/jest --verbose false --runInBand --logHeapUsage --detectOpenHandles",
    "test:dev": "env-cmd -f ./.env.dev jest -i --no-watchman",
    "seed": "env-cmd npx ts-node ./scripts/populateDB.ts up",
    "seed:test": "USE_TEST_DB=true env-cmd npx ts-node ./scripts/populateDB.ts up",
    "seed:dev": "env-cmd -f ./.env.dev npx ts-node ./scripts/populateDB.ts up",
    "seed:test:dev": "USE_TEST_DB=true env-cmd -f ./.env.dev npx ts-node ./scripts/populateDB.ts up",
    "seed:qa": "env-cmd -f ./.env.qa npx ts-node ./scripts/populateDB.ts up",
    "seed:uat": "env-cmd -f ./.env.uat npx ts-node ./scripts/populateDB.ts up",
    "seed:prod": "env-cmd -f ./.env.prod npx ts-node ./scripts/populateDB.ts up",
    "seed:down": "env-cmd npx ts-node ./scripts/populateDB.ts down",
    "seed:down:test": "USE_TEST_DB=true env-cmd npx ts-node ./scripts/populateDB.ts down",
    "seed:down:dev": "env-cmd -f ./.env.dev npx ts-node ./scripts/populateDB.ts down",
    "seed:down:test:dev": "USE_TEST_DB=true env-cmd -f ./.env.dev npx ts-node ./scripts/populateDB.ts down",
    "seed:down:qa": "env-cmd -f ./.env.qa npx ts-node ./scripts/populateDB.ts down",
    "seed:down:uat": "env-cmd -f ./.env.uat npx ts-node ./scripts/populateDB.ts down",
    "seed:down:prod": "env-cmd -f ./.env.prod npx ts-node ./scripts/populateDB.ts down"
  },
  "keywords": [],
  "author": "Kellton",
  "license": "UNLICENSED",
